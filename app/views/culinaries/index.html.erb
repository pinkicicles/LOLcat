<h1>Listing culinaries</h1>

<table>
  <tr>
    <th>Artist</th>
    <th>Name</th>
    <th>Dishes</th>
    <th>Location</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @culinaries.each do |culinary| %>
  <tr>
    <td><%= culinary.Artist %></td>
    <td><%= culinary.Name %></td>
    <td><%= culinary.Dishes %></td>
    <td><%= culinary.Location %></td>
    <td><%= link_to 'Show', culinary %></td>
    <td><%= link_to 'Edit', edit_culinary_path(culinary) %></td>
    <td><%= link_to 'Destroy', culinary, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Culinary', new_culinary_path %>
